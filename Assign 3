#include<stdio.h>
int main()
 {
 int n;
 printf("\nEnter the size of araray\n");
 scanf("%d",&n);
 int a[n],i,j,temp;
 printf("\nEnter the %d array elements",n);
 for(i=0;i<n;i++)
 {
 scanf("%d",&a[i]);
 }
 for(i=0;i<n-1;i++){
 for(j=i+1;j<n;j++) 
 {
  if(a[j] < a[i])
  {
   a[i]=a[i]+a[j];
  a[j]=a[i]-a[j];
  a[i]=a[i]-a[j];
  }
 }
 }
 printf("\nThe sorted array is\n");
 for(i=0;i<n;i++){
 printf("%d\t",a[i]);
 }
 return 0;
 }
