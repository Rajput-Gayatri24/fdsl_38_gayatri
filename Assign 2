#include<stdio.h>
int main()
 {
 int n;
 printf("Enter the size of araray");
 scanf("%d",&n);
 int a[n],i,j,temp;
 printf("Enter the %d array elements",n);
 for(i=0;i<n;i++)
 {
 scanf("%d",&a[i]);
 }
for(i=o;i<n-1;i++){
 for(j=0;j<n-i-1;j++) 
 {
  if(a[j] > a[j+1])
  {
  temp=a[j];
  a[j]=a[j+1];
  a[j+1]=temp;
  }
 }
}
 printf("\nThe sorted array is\n");
 for(i=0;i<n;i++){
 printf("%d\t",a[i]);
 }
 return 0;

 }
